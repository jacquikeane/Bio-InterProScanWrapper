#!/usr/bin/env perl
use Moose;
use Getopt::Long;
use File::Copy;

my($gff,$goterms,$iprlookup,$in,$out,$pathways);
GetOptions(
    'f=s' => \$gff,
    'goterms' => \$goterms,
    'iprlookup' => \$iprlookup,
    'i=s' => \$in,
    'pathways' => \$pathways,
    'outfile=s' => \$out
);

if ($in =~ m/.gff$/) {
    copy('t/data/input_annotation.gff.iprscan.gff', $out);
} else {
    system("touch input_proteins.faa.iprscan.gff");
    system("touch $out");
}

1;
